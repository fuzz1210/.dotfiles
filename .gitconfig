[user]
	name = fuzz1210
	email = fuzz1210@gmail.com
[color]
	ui = auto
[alias]
	cf = config
	cfg = config --global
	lg = log --graph --date=iso --pretty=format:'%C(red)%d %Creset%C(white reverse) %h %Creset%C(green) %an %Creset%C(yellow)%ad [%ar]%Creset%n%n %C(white)%w(80)%s%Creset%n%n%n %w(80,2,2)%b' 
	br = branch
	ch = checkout
	st = status
	df = diff --word-diff -w
	ad = add
	rs = reset HEAD
	cm = commit -m
	ps = push
	fc = fetch
	mg = merge
	pl = pull
[push]
	default = simple
