# .gitconfig



[user]
	name = fuzz1210
	email = fuzz1210@gmail.com
[color]
	ui = auto
[alias]
	cf = config
	cfg = config --global
	lg = log --graph --date=iso --pretty=format:'--------------------------------------------------------------------------------%n%C(white reverse) %h %Creset %C(green)%an%Creset %C(cyan)%ad (%ar)%Creset%C(yellow)%d%Creset%n%n%C(white)%s%Creset%n'
	br = branch
	ch = checkout
	st = status
	df = diff --word-diff -w
	ad = add
	rs = reset HEAD
	cm = commit -m
	ps = push
	fc = fetch
	mg = merge
	pl = pull
[push]
	default = simple
